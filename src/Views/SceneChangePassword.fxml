<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" onMouseClicked="#notificationSetNull" prefHeight="550.0" prefWidth="800.0" style="-fx-background-color: #171717;" stylesheets="@../Css/CssSceneLogOperations.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <PasswordField fx:id="oldPasswordField" layoutX="244.0" layoutY="218.0" onMouseClicked="#notificationSetNull" prefHeight="26.0" prefWidth="311.0" promptText="Password" stylesheets="@../Css/CssTextField.css" />
      <PasswordField fx:id="newPasswordField" layoutX="244.0" layoutY="268.0" onMouseClicked="#notificationSetNull" prefHeight="26.0" prefWidth="311.0" promptText="New Password" />
      <PasswordField fx:id="newPasswordAgainField" layoutX="244.0" layoutY="318.0" onKeyPressed="#enterKeyPressed" onMouseClicked="#notificationSetNull" prefHeight="26.0" prefWidth="311.0" promptText="New Password Again" />
      <Button fx:id="signInButton" layoutX="352.0" layoutY="378.0" mnemonicParsing="false" onAction="#changeButtonPressed" prefHeight="34.0" prefWidth="95.0" style="-fx-background-radius: 12;" stylesheets="@../Css/CssButton.css" text="Change" textAlignment="CENTER">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="mainLabel" alignment="CENTER" layoutX="226.0" layoutY="109.0" prefHeight="58.0" prefWidth="347.0" text="Change Password" textAlignment="CENTER" textFill="#f0f0f0">
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Label fx:id="notificationChangePassword" alignment="CENTER" layoutX="162.0" layoutY="359.0" prefHeight="18.0" prefWidth="475.0" textAlignment="CENTER" textFill="#ff5252">
         <font>
            <Font size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
